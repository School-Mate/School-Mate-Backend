// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:admin@localhost:5432/dev"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       String @id @default(uuid())
  email    String @unique
  password String
  name     String
}
